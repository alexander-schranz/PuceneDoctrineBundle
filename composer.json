{
    "name": "pucene/doctrine-bundle",
    "description": "Integrates pucene in symfony with a doctrine storage layer. Edit",
    "authors": [
        {
            "name": "Pucene Community",
            "homepage": "https://github.com/pucene/PuceneDoctrineBundle/contributors"
        }
    ],
    "keywords": [
        "symfony",
        "bundle",
        "doctrine",
        "lucene",
        "indexing"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "pucene/pucene": "dev-master",
        "pucene/core": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Pucene\\Bundle\\DoctrineBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pucene\\Bundle\\DoctrineBundle\\Tests\\": "tests/"
        }
    }
}

